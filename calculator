from .. import loader, utils
import math

@loader.tds
class CalculatorMod(loader.Module):
"""-–°–ª–æ–∂–µ–Ω–∏–µ (`+`)  
-–í—ã—á–∏—Ç–∞–Ω–∏–µ (`-`)  
-–£–º–Ω–æ–∂–µ–Ω–∏–µ (`*`)  
- –î–µ–ª–µ–Ω–∏–µ (`/`)  
- –¶–µ–ª–æ—á–∏—Å–ª–µ–Ω–Ω–æ–µ –¥–µ–ª–µ–Ω–∏–µ (`//`)  
- –û—Å—Ç–∞—Ç–æ–∫ –æ—Ç –¥–µ–ª–µ–Ω–∏—è (`%`)  
- –í–æ–∑–≤–µ–¥–µ–Ω–∏–µ –≤ —Å—Ç–µ–ø–µ–Ω—å (`**`)  

üìê –ú–∞—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏:  
- sin(x) ‚Äî —Å–∏–Ω—É—Å —É–≥–ª–∞ –≤ —Ä–∞–¥–∏–∞–Ω–∞—Ö  
- cos(x) ‚Äî –∫–æ—Å–∏–Ω—É—Å —É–≥–ª–∞ –≤ —Ä–∞–¥–∏–∞–Ω–∞—Ö  
- tan(x) ‚Äî —Ç–∞–Ω–≥–µ–Ω—Å —É–≥–ª–∞ –≤ —Ä–∞–¥–∏–∞–Ω–∞—Ö  
- sqrt(x) ‚Äî –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã–π –∫–æ—Ä–µ–Ω—å  
- log(x) ‚Äî –Ω–∞—Ç—É—Ä–∞–ª—å–Ω—ã–π –ª–æ–≥–∞—Ä–∏—Ñ–º  
- log10(x) ‚Äî –¥–µ—Å—è—Ç–∏—á–Ω—ã–π –ª–æ–≥–∞—Ä–∏—Ñ–º  
- exp(x) ‚Äî —ç–∫—Å–ø–æ–Ω–µ–Ω—Ç–∞ (e^x)  
- radians(x) ‚Äî –ø—Ä–µ–æ–±—Ä–∞–∑—É–µ—Ç –≥—Ä–∞–¥—É—Å—ã –≤ —Ä–∞–¥–∏–∞–Ω—ã  
- degrees(x) ‚Äî –ø—Ä–µ–æ–±—Ä–∞–∑—É–µ—Ç —Ä–∞–¥–∏–∞–Ω—ã –≤ –≥—Ä–∞–¥—É—Å—ã  
- ceil(x) ‚Äî –æ–∫—Ä—É–≥–ª–µ–Ω–∏–µ –≤–≤–µ—Ä—Ö  
- floor(x) ‚Äî –æ–∫—Ä—É–≥–ª–µ–Ω–∏–µ –≤–Ω–∏–∑  
- factorial(x) ‚Äî —Ñ–∞–∫—Ç–æ—Ä–∏–∞–ª —á–∏—Å–ª–∞  
- gcd(x, y) ‚Äî –Ω–∞–∏–±–æ–ª—å—à–∏–π –æ–±—â–∏–π –¥–µ–ª–∏—Ç–µ–ª—å  
- pow(x, y) ‚Äî –≤–æ–∑–≤–µ–¥–µ–Ω–∏–µ –≤ —Å—Ç–µ–ø–µ–Ω—å (x^y)  
- abs(x) ‚Äî –º–æ–¥—É–ª—å —á–∏—Å–ª–∞  
- round(x, n) ‚Äî –æ–∫—Ä—É–≥–ª–µ–Ω–∏–µ —á–∏—Å–ª–∞ x –¥–æ n –∑–Ω–∞–∫–æ–≤ –ø–æ—Å–ª–µ –∑–∞–ø—è—Ç–æ–π  

üî¢ –ö–æ–Ω—Å—Ç–∞–Ω—Ç—ã:  
- pi ‚Äî —á–∏—Å–ª–æ œÄ (3.14159...)  
- e ‚Äî —á–∏—Å–ª–æ –≠–π–ª–µ—Ä–∞ (2.71828...)"""
strings = {
        "name": "Calculator",
        "usage": "–ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ: .calc <–≤—ã—Ä–∞–∂–µ–Ω–∏–µ>",
        "result": "–†–µ–∑—É–ª—å—Ç–∞—Ç: {}",
    }

    def __init__(self):
        self.name = self.strings["name"]

    @loader.command(ru_doc="–í—ã—á–∏—Å–ª–∏—Ç—å –º–∞—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –≤—ã—Ä–∞–∂–µ–Ω–∏–µ")
    async def calccmd(self, message):
        args = utils.get_args_raw(message)
        if not args:
            await message.edit(self.strings["usage"])
            return

        try:
            result = eval(args, {"__builtins__": None}, {
                "sin": math.sin,
                "cos": math.cos,
                "tan": math.tan,
                "sqrt": math.sqrt,
                "log": math.log,
                "log10": math.log10,
                "exp": math.exp,
                "radians": math.radians,
                "degrees": math.degrees,
                "pi": math.pi,
                "e": math.e,
                "ceil": math.ceil,
                "floor": math.floor,
                "factorial": math.factorial,
                "gcd": math.gcd,
                "pow": math.pow,
                "abs": abs,
                "round": round,
            })
            await message.edit(self.strings["result"].format(result))
        except Exception as e:
            await message.edit(f"–û—à–∏–±–∫–∞: {e}")
