import random
from .. import loader, utils

@loader.tds
class PotyjNomer(loader.Module):
    strings = {
        "name": "PotyjNomer",
        "power_level": (
            "–†—ñ–≤–µ–Ω—å –ø–æ—Ç—É–∂–Ω–æ—Å—Ç—ñ:\n\n"
            "‚¨úÔ∏èüü©üü®üüßüü´üü¶üü™‚¨õÔ∏è‚ò†Ô∏è\n"
            "{dots}üî∫\n\n"
            "–†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –î–û–ô–®–û–í –î–û –í–°–ï–ì–ê–õ–ê–ö–¢–ò–ß–ù–û–ì–û –í–ò–°–û–ö–û–ì–û –†–Ü–í–ù–Ø.\n\n"
            "–ü–†–ò–ë–ï–†–ï–ñ–Ü–¢–¨ –°–í–û–Ñ –ó–î–û–†–û–í º–Ø –Ü –°–í–Ü–ô –ü–û–¢–£–ñ–ù–û–ú–ï–¢–†."
        )
    }

    def __init__(self):
        self.name = self.strings["name"]

    @loader.command(ru_doc="–ü–æ–∫–∞–∑–∞—Ç–∏ —Ä—ñ–≤–µ–Ω—å –ø–æ—Ç—É–∂–Ω–æ—Å—Ç—ñ")
    async def ptjcmd(self, message):
        dots_count = random.randint(0, 40)
        dots = "." * dots_count
        power_message = self.strings["power_level"].format(dots=dots)
        await message.edit(power_message)
