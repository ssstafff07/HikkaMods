    def __init__(self):
        self.name = self.strings["name"]

    @loader.command(ru_doc="–ü–æ–∫–∞–∑–∞—Ç–∏ —Ä—ñ–≤–µ–Ω—å –ø–æ—Ç—É–∂–Ω–æ—Å—Ç—ñ")
    async def ptjcmd(self, message):
        dots_count = random.randint(0, 40)
        dots = "." * dots_count
        phrase = self.strings["phrases"].get(dots_count, "–ù–µ–≤—ñ–¥–æ–º–∏–π —Ä—ñ–≤–µ–Ω—å –ø–æ—Ç—É–∂–Ω–æ—Å—Ç—ñ.")
        power_message = self.strings["power_level"].format(dots=dots, phrase=phrase)
        await message.edit(power_message)
