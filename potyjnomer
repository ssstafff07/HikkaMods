import random
from .. import loader, utils

@loader.tds
class PotyjNomer(loader.Module):
    strings = {
        "name": "PotyjNomer",
        "power_level": (
            "–†—ñ–≤–µ–Ω—å –ø–æ—Ç—É–∂–Ω–æ—Å—Ç—ñ:\n\n"
            "{color}\n"
            "{dots}üî∫\n\n"
            "{message}\n\n"
            "–ü–†–ò–ë–ï–†–ï–ñ–Ü–¢–¨ –°–í–û–Ñ –ó–î–û–†–û–í º–Ø –Ü –°–í–Ü–ô –ü–û–¢–£–ñ–ù–û–ú–ï–¢–†."
        ),
        "messages": {
            "‚¨úÔ∏è": "–ë–Ü–õ–ò–ô –ö–£–ë–Ü–ö: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –ù–ê –ú–Ü–ù–Ü–ú–£–ú–Ü. –í–ò –ú–û–ñ–ï–¢–ï –°–ü–û–ö–Ü–ô–ù–û –†–û–ó–°–õ–ê–ë–ò–¢–ò–°–Ø.",
            "üü©": "–ó–ï–õ–ï–ù–ò–ô –ö–£–ë–Ü–ö: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –°–ï–†–ï–î–ù–Ü–ô. –í–ò –ú–û–ñ–ï–¢–ï –ü–†–û–î–û–í–ñ–£–í–ê–¢–ò –†–û–ë–û–¢–£.",
            "üü®": "–ñ–û–í–¢–ò–ô –ö–£–ë–Ü–ö: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –¢–†–û–•–ò –ü–Ü–î–í–ò–©–ï–ù–ò–ô. –ë–£–î–¨–¢–ï –£–í–ê–ñ–ù–ò–ú–ò.",
            "üüß": "–û–†–ê–ù–ñ–ï–í–ò–ô –ö–£–ë–Ü–ö: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –í–ò–°–û–ö–ò–ô. –ü–†–ò–ô–ú–Ü–¢–¨ –ó–ê–•–û–î–ò –î–õ–Ø –ó–ù–ò–ñ–ï–ù–ù–Ø –°–¢–†–ï–°–£.",
            "üü´": "–ö–û–†–ò–ß–ù–ï–í–ò–ô –ö–£–ë–Ü–ö: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –î–£–ñ–ï –í–ò–°–û–ö–ò–ô. –í–ê–ú –ü–û–¢–†–Ü–ë–ï–ù –í–Ü–î–ü–û–ß–ò–ù–û–ö.",
            "üü¶": "–°–ò–ù–Ü–ô –ö–£–ë–Ü–ö: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –ö–†–ò–¢–ò–ß–ù–ò–ô. –ù–ï–ì–ê–ô–ù–û –ó–£–ü–ò–ù–Ü–¢–¨–°–Ø –Ü –ó–ê–ô–ú–Ü–¢–¨–°–Ø –°–û–ë–û–Æ.",
            "üü™": "–§–Ü–û–õ–ï–¢–û–í–ò–ô –ö–£–ë–Ü–ö: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –ó–ê–ú–ï–ñ–ù–ò–ô. –í–ê–ú –ü–û–¢–†–Ü–ë–ù–ê –î–û–ü–û–ú–û–ì–ê.",
            "‚¨õÔ∏è": "–ß–û–†–ù–ò–ô –ö–£–ë–Ü–ö: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –ù–ï–í–Ü–î–û–ú–ò–ô. –©–û–°–¨ –ü–Ü–®–õ–û –ù–ï –¢–ê–ö.",
            "‚ò†Ô∏è": "–ß–ï–†–ï–ü: –†–Ü–í–ï–ù–¨ –ü–û–¢–£–ñ–ù–û–°–¢–Ü –î–û–ô–®–û–í –î–û –ö–†–ò–¢–ò–ß–ù–û–á –¢–û–ß–ö–ò. –í–ò–ú–ö–ù–Ü–¢–¨ –£–°–ï –Ü –í–Ü–î–ü–û–ß–ò–í–ê–ô–¢–ï."
        }
    }

    def __init__(self):
        self.name = self.strings["name"]

    @loader.command(ru_doc="–ü–æ–∫–∞–∑–∞—Ç–∏ —Ä—ñ–≤–µ–Ω—å –ø–æ—Ç—É–∂–Ω–æ—Å—Ç—ñ")
    async def ptjcmd(self, message):
        colors = ["‚¨úÔ∏è", "üü©", "üü®", "üüß", "üü´", "üü¶", "üü™", "‚¨õÔ∏è", "‚ò†Ô∏è"]
        color = random.choice(colors)
        color_message = self.strings["messages"][color]
        dots_count = random.randint(0, 40)
        dots = "." * dots_count
        power_message = self.strings["power_level"].format(
            color=color,
            dots=dots,
            message=color_message
        )
        await message.edit(power_message)
