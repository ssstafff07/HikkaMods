import speedtest
from .. import loader, utils

@loader.tds
class SpeedTestModule(loader.Module):
    """–ú–æ–¥—É–ª—å –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏–Ω—Ç–µ—Ä–Ω–µ—Ç–∞, –ø–∏–Ω–≥–∞ –∏ —Å–∫–æ—Ä–æ—Å—Ç–∏ —Å–∫–∞—á–∏–≤–∞–Ω–∏—è."""
    strings = {
        "name": "SpeedTest",
        "testing": "‚è≥ –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å –∏–Ω—Ç–µ—Ä–Ω–µ—Ç–∞...",
        "result": (
            "üìä –†–µ–∑—É–ª—å—Ç–∞—Ç—ã —Ç–µ—Å—Ç–∞ —Å–∫–æ—Ä–æ—Å—Ç–∏:\n\n"
            "üõú **–°–∫–æ—Ä–æ—Å—Ç—å –∑–∞–≥—Ä—É–∑–∫–∏:** {download} Mbps\n"
            "üì§ **–°–∫–æ—Ä–æ—Å—Ç—å –æ—Ç–¥–∞—á–∏:** {upload} Mbps\n"
            "üèì **–ü–∏–Ω–≥:** {ping} ms\n\n"
            "üåê **–°–µ—Ä–≤–µ—Ä:** {server}\n"
            "üìç **–õ–æ–∫–∞—Ü–∏—è —Å–µ—Ä–≤–µ—Ä–∞:** {location}"
        )
    }

    def __init__(self):
        self.name = self.strings["name"]

    @loader.command(ru_doc="–ü—Ä–æ–≤–µ—Ä–∏—Ç—å —Å–∫–æ—Ä–æ—Å—Ç—å –∏–Ω—Ç–µ—Ä–Ω–µ—Ç–∞")
    async def speedtestcmd(self, message):
        """–ü—Ä–æ–≤–µ—Ä—è–µ—Ç —Å–∫–æ—Ä–æ—Å—Ç—å –∏–Ω—Ç–µ—Ä–Ω–µ—Ç–∞, –ø–∏–Ω–≥ –∏ —Å–∫–æ—Ä–æ—Å—Ç—å —Å–∫–∞—á–∏–≤–∞–Ω–∏—è."""
        await message.edit(self.strings["testing"])

       
        st = speedtest.Speedtest()

        
        st.get_best_server()

        download_speed = st.download() / 1_000_000  # –ü–µ—Ä–µ–≤–æ–¥–∏–º –≤ Mbps
        upload_speed = st.upload() / 1_000_000  # –ü–µ—Ä–µ–≤–æ–¥–∏–º –≤ Mbps
        ping = st.results.ping  
        server = st.results.server["sponsor"]
        location = f"{st.results.server['name']}, {st.results.server['
        result_message = self.strings["result"].format(
            download=round(download_speed, 2),
            upload=round(upload_speed, 2),
            ping=round(ping, 2),
            server=server,
            location=location
        )

        
        await message.edit(result_message)
