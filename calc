from .. import loader, utils
import math

@loader.tds
class CalculatorMod(loader.Module):
    """–ö–∞–ª—å–∫—É–ª—è—Ç–æ—Ä —Å –ø–æ–¥–¥–µ—Ä–∂–∫–æ–π –º–∞—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–∏—Ö –æ–ø–µ—Ä–∞—Ü–∏–π –∏ —Ñ—É–Ω–∫—Ü–∏–π."""
    strings = {
        "name": "Calculator",
        "usage": "–ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ: .calc <–≤—ã—Ä–∞–∂–µ–Ω–∏–µ>",
        "result": "–†–µ–∑—É–ª—å—Ç–∞—Ç: {}",
        "description": (
            "üìö **–î–æ—Å—Ç—É–ø–Ω—ã–µ –æ–ø–µ—Ä–∞—Ü–∏–∏ –∏ —Ñ—É–Ω–∫—Ü–∏–∏:**\n\n"
            "‚ûï **–û—Å–Ω–æ–≤–Ω—ã–µ –æ–ø–µ—Ä–∞—Ü–∏–∏:**\n"
            "- –°–ª–æ–∂–µ–Ω–∏–µ (`+`)\n"
            "- –í—ã—á–∏—Ç–∞–Ω–∏–µ (`-`)\n"
            "- –£–º–Ω–æ–∂–µ–Ω–∏–µ (`*`)\n"
            "- –î–µ–ª–µ–Ω–∏–µ (`/`)\n"
            "- –¶–µ–ª–æ—á–∏—Å–ª–µ–Ω–Ω–æ–µ –¥–µ–ª–µ–Ω–∏–µ (`//`)\n"
            "- –û—Å—Ç–∞—Ç–æ–∫ –æ—Ç –¥–µ–ª–µ–Ω–∏—è (`%`)\n"
            "- –í–æ–∑–≤–µ–¥–µ–Ω–∏–µ –≤ —Å—Ç–µ–ø–µ–Ω—å (`**`)\n\n"
            "üìê **–ú–∞—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏:**\n"
            "- `sin(x)` ‚Äî —Å–∏–Ω—É—Å —É–≥–ª–∞ –≤ —Ä–∞–¥–∏–∞–Ω–∞—Ö\n"
            "- `cos(x)` ‚Äî –∫–æ—Å–∏–Ω—É—Å —É–≥–ª–∞ –≤ —Ä–∞–¥–∏–∞–Ω–∞—Ö\n"
            "- `tan(x)` ‚Äî —Ç–∞–Ω–≥–µ–Ω—Å —É–≥–ª–∞ –≤ —Ä–∞–¥–∏–∞–Ω–∞—Ö\n"
            "- `sqrt(x)` ‚Äî –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã–π –∫–æ—Ä–µ–Ω—å\n"
            "- `log(x)` ‚Äî –Ω–∞—Ç—É—Ä–∞–ª—å–Ω—ã–π –ª–æ–≥–∞—Ä–∏—Ñ–º\n"
            "- `log10(x)` ‚Äî –¥–µ—Å—è—Ç–∏—á–Ω—ã–π –ª–æ–≥–∞—Ä–∏—Ñ–º\n"
            "- `exp(x)` ‚Äî —ç–∫—Å–ø–æ–Ω–µ–Ω—Ç–∞ (e^x)\n"
            "- `radians(x)` ‚Äî –ø—Ä–µ–æ–±—Ä–∞–∑—É–µ—Ç –≥—Ä–∞–¥—É—Å—ã –≤ —Ä–∞–¥–∏–∞–Ω—ã\n"
            "- `degrees(x)` ‚Äî –ø—Ä–µ–æ–±—Ä–∞–∑—É–µ—Ç —Ä–∞–¥–∏–∞–Ω—ã –≤ –≥—Ä–∞–¥—É—Å—ã\n"
            "- `ceil(x)` ‚Äî –æ–∫—Ä—É–≥–ª–µ–Ω–∏–µ –≤–≤–µ—Ä—Ö\n"
            "- `floor(x)` ‚Äî –æ–∫—Ä—É–≥–ª–µ–Ω–∏–µ –≤–Ω–∏–∑\n"
            "- `factorial(x)` ‚Äî —Ñ–∞–∫—Ç–æ—Ä–∏–∞–ª —á–∏—Å–ª–∞\n"
            "- `gcd(x, y)` ‚Äî –Ω–∞–∏–±–æ–ª—å—à–∏–π –æ–±—â–∏–π –¥–µ–ª–∏—Ç–µ–ª—å\n"
            "- `pow(x, y)` ‚Äî –≤–æ–∑–≤–µ–¥–µ–Ω–∏–µ –≤ —Å—Ç–µ–ø–µ–Ω—å (x^y)\n"
            "- `abs(x)` ‚Äî –º–æ–¥—É–ª—å —á–∏—Å–ª–∞\n"
            "- `round(x, n)` ‚Äî –æ–∫—Ä—É–≥–ª–µ–Ω–∏–µ —á–∏—Å–ª–∞ `x` –¥–æ `n` –∑–Ω–∞–∫–æ–≤ –ø–æ—Å–ª–µ –∑–∞–ø—è—Ç–æ–π\n\n"
            "üî¢ **–ö–æ–Ω—Å—Ç–∞–Ω—Ç—ã:**\n"
            "- `pi` ‚Äî —á–∏—Å–ª–æ œÄ (3.14159...)\n"
            "- `e` ‚Äî —á–∏—Å–ª–æ –≠–π–ª–µ—Ä–∞ (2.71828...)\n\n"
          
        )
    }

    def __init__(self):
        self.name = self.strings["name"]

    @loader.command(ru_doc="–í—ã—á–∏—Å–ª–∏—Ç—å –º–∞—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –≤—ã—Ä–∞–∂–µ–Ω–∏–µ")
    async def calccmd(self, message):
        args = utils.get_args_raw(message)
        if not args:
            await message.edit(self.strings["usage"])
            return

        try:
            result = eval(args, {"__builtins__": None}, {
                "sin": math.sin,
                "cos": math.cos,
                "tan": math.tan,
                "sqrt": math.sqrt,
                "log": math.log,
                "log10": math.log10,
                "exp": math.exp,
                "radians": math.radians,
                "degrees": math.degrees,
                "pi": math.pi,
                "e": math.e,
                "ceil": math.ceil,
                "floor": math.floor,
                "factorial": math.factorial,
                "gcd": math.gcd,
                "pow": math.pow,
                "abs": abs,
                "round": round,
            })
            await message.edit(self.strings["result"].format(result))
        except Exception as e:
            await message.edit(f"–û—à–∏–±–∫–∞: {e}")
